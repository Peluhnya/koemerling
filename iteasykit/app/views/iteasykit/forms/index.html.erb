<p id="notice"><%= notice %></p>

<h1>Forms</h1>

<table>
  <thead>
    <tr>
      <th>Iteasykit entity type</th>
      <th>Active</th>
      <th>Sticky</th>
      <th>Position</th>
      <th>Group</th>
      <th>Machine name</th>
      <th>Iteasykit seomore</th>
      <th>Slug</th>
      <th>Calc</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @forms.each do |form| %>
      <tr>
        <td><%= form.iteasykit_entity_type %></td>
        <td><%= form.active %></td>
        <td><%= form.sticky %></td>
        <td><%= form.position %></td>
        <td><%= form.group %></td>
        <td><%= form.machine_name %></td>
        <td><%= form.iteasykit_seomore %></td>
        <td><%= form.slug %></td>
        <td><%= form.calc %></td>
        <td><%= link_to 'Show', form %></td>
        <td><%= link_to 'Edit', edit_form_path(form) %></td>
        <td><%= link_to 'Destroy', form, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Form', new_form_path %>
