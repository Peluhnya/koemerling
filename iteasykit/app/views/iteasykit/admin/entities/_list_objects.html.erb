
  <p><%= t(:help_list_entity) %></p>

<table class="table table-bordered table-hover table-striped">
  <thead>
  <tr>
    <th><%= t('id') %></th>
    <th><%= t('title') %></th>
    <th><%= t('machine_name') %></th>
    <th><%= t('active') %></th>
    <th><%= t('sticky') %></th>
    <th><%= t('action') %></th>
  </tr>
  </thead>

  <tbody>
  <% @object_list.each do |entity| %>
    <tr>
      <td><%= entity.id %></td>
      <td><%= entity.title %></td>
      <td><%= entity.machine_name %></td>
      <td><i class="zmdi zmdi-<%= entity.active ? 'check' : 'close' %>"></i></td>
      <td><i class="zmdi zmdi-<%= entity.sticky ? 'check' : 'close' %>"></i></td>
      <td class="text-center">

        <%= link_to '<i class="zmdi zmdi-edit"></i>'.html_safe, edit_admin_entity_path(entity), class: "btn btn-success raised icon waves-effect" %>
        <%= link_to '<i class="zmdi zmdi-delete"></i>'.html_safe, [:admin, entity], class: "btn btn-danger raised icon waves-effect", method: :delete,
                    data: {confirm: 'Are you sure?'} %>     </td>

    </tr>
  <% end %>
  </tbody>
</table>