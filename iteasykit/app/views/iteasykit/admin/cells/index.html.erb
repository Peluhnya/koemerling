<p id="notice"><%= notice %></p>

<h1>Cells</h1>

<table>
  <thead>
    <tr>
      <th>Iteasykit entity type</th>
      <th>Active</th>
      <th>Sticky</th>
      <th>Position</th>
      <th>Group</th>
      <th>Machine name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cells.each do |cell| %>
      <tr>
        <td><%= cell.iteasykit_entity_type %></td>
        <td><%= cell.active %></td>
        <td><%= cell.sticky %></td>
        <td><%= cell.position %></td>
        <td><%= cell.group %></td>
        <td><%= cell.machine_name %></td>
        <td><%= link_to 'Show', [:admin, cell] %></td>
        <td><%= link_to 'Edit', edit_admin_cell_path(cell) %></td>
        <td><%= link_to 'Destroy', [:admin, cell], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cell', new_admin_cell_path %>
