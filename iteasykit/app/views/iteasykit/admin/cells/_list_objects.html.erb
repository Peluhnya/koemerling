
<table class="table table-bordered table-hover table-striped">
  <thead>
  <tr>
    <th><%= t('id') %></th>
    <th><%= t('active') %></th>
    <th><%= t('sticky') %></th>
    <th><%= t('position') %></th>
    <th><%= t('group') %></th>
    <th><%= t('machine_name') %></th>
    <th><%= t('action') %></th>
  </tr>
  </thead>

  <tbody>
  <% @object_list.each do |cell| %>
    <tr>
      <td><%= cell.id %></td>
      <td><i class="zmdi zmdi-<%= cell.active ? 'check' : 'close' %>"></i></td>
      <td><i class="zmdi zmdi-<%= cell.sticky ? 'check' : 'close' %>"></i></td>
      <td><%= cell.position %></td>
      <td><%= cell.group %></td>
      <td><%= cell.machine_name %></td>
      <td class="text-center">
        <%= link_to '<i class="zmdi zmdi-edit"></i>'.html_safe, edit_admin_cell_path(cell), class: "btn btn-success raised icon waves-effect" %>
        <%= link_to '<i class="zmdi zmdi-delete"></i>'.html_safe, [:admin, cell], class: "btn btn-danger raised icon waves-effect", method: :delete,
                    data: {confirm: 'Are you sure?'} %>     </td>

    </tr>
  <% end %>
  </tbody>
</table>